<div class="team">
  <h1><%= @team.name %></h1><br/>
  <h2>Team Motto: <%= @team.motto %></h2>
</div><br>

<% @members.each do |hero| %>
   <div class="superhero">
     <h2>Hero Name: <%= hero.name %></h2>
     <li>Hero Power: <%= hero.power %></li>
     <li>Hero Biography: <%= hero.bio %></li>
   </div><br>
<% end %>
